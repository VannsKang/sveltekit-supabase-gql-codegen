		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.2.6",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@supabase/supabase-js": "^2.7.1",
		"graphql": "^16.6.0",
		"graphql-request": "^5.1.0",
		"graphql-yoga": "^3.5.1"
